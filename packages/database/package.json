{
  "name": "@backy-cms/database",
  "version": "1.0.0",
  "description": "Database client and queries for Backy CMS (Supabase)",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --external @supabase/supabase-js",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch --external @supabase/supabase-js",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@backy-cms/core": "*",
    "@supabase/supabase-js": "^2.39.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "cms",
    "database",
    "supabase"
  ],
  "author": "Backy CMS Team",
  "license": "MIT"
}