{
  "name": "backy",
  "version": "1.0.0",
  "description": "Backy - Free, open-source visual website builder and CMS backend",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean && rm -rf node_modules",
    "db:generate": "cd packages/db && npx drizzle-kit generate",
    "db:migrate": "cd packages/db && npx drizzle-kit migrate",
    "db:studio": "cd packages/db && npx drizzle-kit studio",
    "setup": "bash scripts/setup.sh"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/s3-request-presigner": "^3.0.0",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@libsql/client": "^0.4.0",
    "@supabase/supabase-js": "^2.39.0",
    "@types/node": "^20.0.0",
    "@udecode/plate-paragraph": "latest",
    "better-sqlite3": "^9.0.0",
    "mysql2": "^3.0.0",
    "postgres": "^3.4.0",
    "stripe": "^14.0.0",
    "turbo": "^2.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "npm@10.0.0",
  "author": "Backy CMS Team (Built by Kimi 2.5)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/backy-cms/backy"
  },
  "keywords": [
    "cms",
    "website-builder",
    "visual-editor",
    "open-source",
    "free",
    "wix-alternative",
    "react",
    "typescript",
    "supabase"
  ]
}
